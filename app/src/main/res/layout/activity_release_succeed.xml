<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mredrock.cyxbs.ui.activity.lost.ReleaseSucceedActivity">
    <include layout="@layout/toolbar"/>
    <ImageView
        android:id="@+id/image_s"
        android:layout_centerInParent="true"
        android:background="@drawable/img_lost_post_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_centerHorizontal="true"
        android:layout_margin="20dp"
        android:layout_below="@+id/image_s"
        android:text="青协的干事们还在努力审核您的信息，请稍等哟~"
        android:textSize="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>
